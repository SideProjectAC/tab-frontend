{
  "manifest_version": 3,
  "name": "react tab",
  "version": "1.0",
   "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabGroups",
    "tabs",
    "favicon"
  ],
  "web_accessible_resources": [ 
        { 
            "resources": ["_favicon/*"], 
            "matches": ["<all_urls>"], 
            "extension_ids": ["*"] 
        } 
  ],
  "background": {
    "service_worker": "background.js"
  }
}